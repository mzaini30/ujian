{% extends 'layout/default.twig' %}

{% block isi %}
    {% for x in data %}
<div class="tampil">
       	<p>Nama: {{ x.nama }}</p>
       	<p>Kelas: {{ x.kelas }}</p>
       	<p><strong>Jawaban 1</strong></p>
       	<p>{{ x.jawaban1 }}</p>
       	<p><strong>Jawaban 2</strong></p>
       	<p>{{ x.jawaban2 }}</p>
       	<p><strong>Jawaban 3</strong></p>
       	<p>{{ x.jawaban3 }}</p>
       	<p><strong>Jawaban 4</strong></p>
       	<p>{{ x.jawaban4 }}</p>
       	<p><strong>Jawaban 5</strong></p>
       	<p>{{ x.jawaban5 }}</p>
       	<p><strong>Jawaban 6</strong></p>
       	<p>{{ x.jawaban6 }}</p>
       	<p><strong>Jawaban 7</strong></p>
       	<p>{{ x.jawaban7 }}</p>
       	<p><strong>Jawaban 8</strong></p>
       	<p>{{ x.jawaban8 }}</p>
       	<p><strong>Jawaban 9</strong></p>
       	<p>{{ x.jawaban9 }}</p>
       	<p><strong>Jawaban 10</strong></p>
       	<p>{{ x.jawaban10 }}</p>
       	<hr>
</div>
    {% endfor %}
{% endblock %}

{% block head %}
  <style>
    .tampil p {
      white-space: pre-wrap;
    }
  </style>
{% endblock %}